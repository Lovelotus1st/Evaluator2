<!DOCTYPE html>
<html>
<head>
  <title>SurveyEngine</title>
  <%= render "layouts/stylesheets" %>
  <%= stylesheet_link_tag :all %>
  <%= stylesheet_link_tag "survey" , :media => "all" %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "jquery-1.4.4.min" %>
  <%= javascript_include_tag "jquery-ui" %>
  <%= javascript_include_tag "editQuestionsPage" %>
  <%= javascript_include_tag "radioButtonHandler" %>
  <%= javascript_include_tag "verification" %>
  <%= javascript_include_tag "barchartGrowth" %>
  <%= csrf_meta_tag %>

</head>

<body class="<%= controller.controller_name %>">
  
    <div id="banner">
    <%= render "layouts/header", :locals => { :page_header => @page_header } %>
    </div>
    <div id="container">
    <div id="main-content">
      <%= yield %>
   </div>
</body>

</html>

